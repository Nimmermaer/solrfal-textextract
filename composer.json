{
  "name": "beechit/solrfal-textextract",
  "type": "typo3-cms-extension",
  "description": "Add text extracting for SOLR indexing of FileAbstractionLayer based files in TYPO3 CMS",
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "php" : ">=5.3.7",
    "typo3/cms-core": "^10.4.10",
    "apache-solr-for-typo3/solrfal": ">4"
  },
  "autoload": {
    "psr-4": {
      "BeechIt\\SolrfalTextextract\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "solrfal_textextract"
    }
  }
}
